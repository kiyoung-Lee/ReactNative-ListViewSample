{
    "naning9": {
    "url": "http://www.naning9.com/",
    "root_tag_list": [
      {
        "tag":"div",
        "class": {
          "class" : "item_list cle"
        },
        "is_container" : false,
        "grid_name_key" : "grid_name",
        "item_code_pattern" : "item_code",
        "item_detail_url_pattern" : "item_detail_url",
        "image_url_pattern" :"image_url",
        "item_name_pattern": "item_name",
        "item_price_pattern": "item_price"
      }
    ],
    "cell_name":{
      "tag":"div",
      "class":{
        "class": "list_cell"
      }
    },
    "pattern":{
      "item_code":[
        {
          "command":"find",
          "tag":"a"
        },
        {
          "command":"element",
          "tag":"href"
        }
      ],
      "item_detail_url":[
        {
          "command":"find",
          "tag":"a"
        },
        {
          "command":"element",
          "tag":"href"
        }
      ],
      "image_url":[
        {
          "command":"find",
          "tag":"a"
        },
        {
          "command":"find",
          "tag":"img"
        },
        {
          "command":"element",
          "tag":"src"
        }
      ],
      "item_name":[
        {
          "command":"find",
          "tag":"ul"
        },
        {
          "command":"find_class",
          "tag":"li",
          "class":{
            "class" : "item_name dsc_gray"
          }
        },
        {
          "command":"text",
          "tag":"text"
        }
      ],
      "item_price":[
        {
          "command":"find",
          "tag":"ul"
        },
        {
          "command":"find_all_class",
          "tag":"li",
          "class":{
            "class" : "item_price"
          }
        },
        {
          "command":"element",
          "tag":1
        },
        {
          "command":"find",
          "tag":"p"
        },
        {
          "command":"text",
          "tag":"text"
        }
      ]
    },
    "filter":{
      "item_code":[
        {
          "command" : "find",
          "parameters" :[
            "index_no"
          ]
        },
        {
          "command" : "find",
          "parameters" :[
            "&"
          ]
        },
        {
          "command" : "plus_back_get_result",
          "parameters" :[
            0,9
          ]
        },
        {
          "command" : "substring_get_result",
          "parameters" :[
            2,1
          ]
        }
      ],
      "detail_view_url":[
        {
          "command":"plus_front",
          "parameters" :[
            "http://www.naning9.com"
          ]
        }
      ],
      "item_name":[
        {
          "command": "encode",
          "parameters" :[
            "utf-8"
          ]
        }
      ]
    }
  }
}